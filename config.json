  "price_apis": {
    "coingecko": {
      "name": "CoinGecko",
      "base_url": "https://api.coingecko.com/api/v3",
      "endpoints": {
        "simple_price": "/simple/price?ids={ids}&vs_currencies={currencies}",
        "coin_data": "/coins/{id}",
        "token_price": "/simple/token_price/{platform}?contract_addresses={addresses}&vs_currencies={currencies}"
      },
      "rate_limit": "50 calls/minute",
      "requires_key": false,
      "platform_ids": {
        "ethereum": "ethereum",
        "polygon": "polygon-pos",
        "arbitrum": "arbitrum-one",
        "optimism": "optimistic-ethereum",
        "base": "base",
        "bsc": "binance-smart-chain",
        "avalanche": "avalanche",
        "fantom": "fantom",
        "solana": "solana"
      }
    },
    "coincap": {
      "name": "CoinCap",
      "base_url": "https://api.coincap.io/v2",
      "endpoints": {
        "assets": "/assets",
        "asset": "/assets/{id}",
        "rates": "/rates"
      },
      "rate_limit": "200 calls/minute",
      "requires_key": false
    },
    "cryptocompare": {
      "name": "CryptoCompare",
      "base_url": "https://min-api.cryptocompare.com/data",
      "endpoints": {
        "price": "/price?fsym={from}&tsyms={to}",
        "pricemulti": "/pricemulti?fsyms={from}&tsyms={to}",
        "historical": "/pricehistorical?fsym={from}&tsyms={to}&ts={timestamp}"
      },
      "rate_limit": "100,000 calls/month (free)",
      "requires_key": false
    },
    "debank": {
      "name": "DeBank",
      "base_url": "https://api.debank.com",
      "endpoints": {
        "token_list": "/token/cache_balance_list?user_addr={address}",
        "user_total": "/user/total_balance?id={address}",
        "chain_list": "/user/chain_balance?id={address}",
        "portfolio": "/portfolio/project_list?user_addr={address}"
      },
      "rate_limit": "Limited (varies)",
      "requires_key": false,
      "note": "Some endpoints may require API key for heavy usage"
    }
  },


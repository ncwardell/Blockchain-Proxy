  "method_documentation": {
    "eth_blockNumber": {
      "params": [],
      "description": "Get the current block number (latest block height on the chain)",
      "returns": "hex string (e.g., '0x1234567')",
      "chain_types": [
        "evm"
      ]
    },
    "eth_chainId": {
      "params": [],
      "description": "Get the chain ID for transaction signing and network identification",
      "returns": "hex string (e.g., '0x1' for Ethereum mainnet)",
      "chain_types": [
        "evm"
      ]
    },
    "eth_gasPrice": {
      "params": [],
      "description": "Get the current gas price in Wei for transaction cost estimation",
      "returns": "hex string (wei amount, e.g., '0x4a817c800' = 20 Gwei)",
      "chain_types": [
        "evm"
      ]
    },
    "eth_getBalance": {
      "params": [
        "address (0x...)",
        "block ('latest', 'earliest', 'pending', or hex block number)"
      ],
      "description": "Get the native token balance (ETH, MATIC, AVAX, etc.) for an address",
      "returns": "hex string (balance in wei)",
      "chain_types": [
        "evm"
      ]
    },
    "eth_getTransactionCount": {
      "params": [
        "address",
        "block"
      ],
      "description": "Get the nonce (transaction count) for an address - required for sending transactions",
      "returns": "hex string (nonce value)",
      "chain_types": [
        "evm"
      ]
    },
    "eth_getCode": {
      "params": [
        "address",
        "block"
      ],
      "description": "Get the bytecode at an address (used to verify if address is a contract)",
      "returns": "hex string (contract bytecode, or '0x' if not a contract)",
      "chain_types": [
        "evm"
      ]
    },
    "eth_getStorageAt": {
      "params": [
        "address",
        "position (hex)",
        "block"
      ],
      "description": "Get the value from a storage position at a given address",
      "returns": "hex string (storage value)",
      "chain_types": [
        "evm"
      ]
    },
    "eth_call": {
      "params": [
        "{to, data, from?, gas?, gasPrice?, value?}",
        "block"
      ],
      "description": "Execute a read-only contract call without creating a transaction (for querying contract state)",
      "returns": "hex string (ABI-encoded return data)",
      "chain_types": [
        "evm"
      ]
    },
    "eth_estimateGas": {
      "params": [
        "{to, data, from?, gas?, gasPrice?, value?}"
      ],
      "description": "Estimate gas required for a transaction (important for avoiding out-of-gas errors)",
      "returns": "hex string (estimated gas units)",
      "chain_types": [
        "evm"
      ]
    },
    "eth_getBlockByNumber": {
      "params": [
        "block (hex or 'latest')",
        "fullTx (boolean: true for full tx objects, false for hashes only)"
      ],
      "description": "Get full block data including transactions, timestamp, and miner info",
      "returns": "block object with transactions array",
      "chain_types": [
        "evm"
      ]
    },
    "eth_getBlockByHash": {
      "params": [
        "blockHash (0x...)",
        "fullTx (boolean)"
      ],
      "description": "Get block data by its hash rather than number",
      "returns": "block object",
      "chain_types": [
        "evm"
      ]
    },
    "eth_getTransactionByHash": {
      "params": [
        "txHash (0x...)"
      ],
      "description": "Get transaction details by hash (status, from, to, value, gas, etc.)",
      "returns": "transaction object or null if not found",
      "chain_types": [
        "evm"
      ]
    },
    "eth_getTransactionReceipt": {
      "params": [
        "txHash"
      ],
      "description": "Get transaction receipt with execution status, logs, and gas used (only available after tx is mined)",
      "returns": "receipt object with status (0x1=success, 0x0=failed), logs, gasUsed, etc.",
      "chain_types": [
        "evm"
      ]
    },
    "eth_getLogs": {
      "params": [
        "{fromBlock?, toBlock?, address?, topics?}"
      ],
      "description": "Query event logs emitted by contracts (essential for monitoring contract activity)",
      "returns": "array of log objects",
      "chain_types": [
        "evm"
      ]
    },
    "eth_sendRawTransaction": {
      "params": [
        "signedTx (hex string of signed transaction)"
      ],
      "description": "Broadcast a signed transaction to the network",
      "returns": "transaction hash (0x...)",
      "chain_types": [
        "evm"
      ]
    },
    "net_version": {
      "params": [],
      "description": "Get the network ID",
      "returns": "string (network ID)",
      "chain_types": [
        "evm"
      ]
    },
    "web3_clientVersion": {
      "params": [],
      "description": "Get the client version string",
      "returns": "string (client version)",
      "chain_types": [
        "evm"
      ]
    },
    "getBalance": {
      "params": [
        "publicKey (base58 string)",
        "optional: {commitment, minContextSlot}"
      ],
      "description": "Get SOL balance for a Solana account in lamports (1 SOL = 1,000,000,000 lamports)",
      "returns": "object: {context: {slot}, value: lamports}",
      "chain_types": [
        "solana"
      ]
    },
    "getBlockHeight": {
      "params": [
        "optional: {commitment, minContextSlot}"
      ],
      "description": "Get current block height on Solana",
      "returns": "number (block height)",
      "chain_types": [
        "solana"
      ]
    },
    "getTransaction": {
      "params": [
        "signature (base58)",
        "optional: {encoding, commitment, maxSupportedTransactionVersion}"
      ],
      "description": "Get transaction details by signature",
      "returns": "transaction object with meta, blockTime, and slot info",
      "chain_types": [
        "solana"
      ]
    },
    "getLatestBlockhash": {
      "params": [
        "optional: {commitment, minContextSlot}"
      ],
      "description": "Get latest blockhash and last valid block height (required for transaction creation)",
      "returns": "object: {blockhash, lastValidBlockHeight}",
      "chain_types": [
        "solana"
      ]
    },
    "getTokenAccountsByOwner": {
      "params": [
        "owner (pubkey)",
        "{mint: tokenMintAddress} or {programId: tokenProgramId}",
        "optional: {encoding, dataSlice, minContextSlot}"
      ],
      "description": "Get all token accounts owned by an address for a specific SPL token",
      "returns": "array of token account objects with balances",
      "chain_types": [
        "solana"
      ]
    },
    "sendTransaction": {
      "params": [
        "signedTransaction (base64)",
        "optional: {skipPreflight, preflightCommitment, encoding, maxRetries, minContextSlot}"
      ],
      "description": "Broadcast a signed transaction to Solana",
      "returns": "transaction signature",
      "chain_types": [
        "solana"
      ]
    },
    "getAccountInfo": {
      "params": [
        "publicKey",
        "optional: {commitment, encoding, dataSlice}"
      ],
      "description": "Get account data and metadata for any Solana account",
      "returns": "account object with lamports, owner, data, executable status",
      "chain_types": [
        "solana"
      ]
    },
    "getBlock": {
      "params": [
        "slot",
        "optional: {encoding, transactionDetails, rewards, commitment, maxSupportedTransactionVersion}"
      ],
      "description": "Get block data for a specific slot",
      "returns": "block object with transactions and metadata",
      "chain_types": [
        "solana"
      ]
    },
    "getBlockTime": {
      "params": [
        "slot"
      ],
      "description": "Get estimated production time of a block",
      "returns": "unix timestamp or null",
      "chain_types": [
        "solana"
      ]
    },
    "getEpochInfo": {
      "params": [
        "optional: {commitment, minContextSlot}"
      ],
      "description": "Get information about the current epoch",
      "returns": "object with epoch, slotIndex, slotsInEpoch, etc.",
      "chain_types": [
        "solana"
      ]
    },
    "getblockcount": {
      "params": [],
      "description": "Get the current block height on Bitcoin blockchain",
      "returns": "number (block height)",
      "chain_types": [
        "bitcoin"
      ]
    },
    "getbestblockhash": {
      "params": [],
      "description": "Get hash of the tip (latest) block",
      "returns": "hex string (block hash)",
      "chain_types": [
        "bitcoin"
      ]
    },
    "getrawtransaction": {
      "params": [
        "txid (hex string)",
        "verbose (boolean: true for decoded JSON, false for raw hex)"
      ],
      "description": "Get raw transaction data by transaction ID",
      "returns": "hex string (if verbose=false) or transaction object (if verbose=true)",
      "chain_types": [
        "bitcoin"
      ]
    },
    "sendrawtransaction": {
      "params": [
        "hexstring (signed transaction)"
      ],
      "description": "Broadcast a signed Bitcoin transaction to the network",
      "returns": "transaction ID (txid)",
      "chain_types": [
        "bitcoin"
      ]
    },
    "getblock": {
      "params": [
        "blockhash",
        "verbosity (0=hex, 1=json, 2=json with tx details)"
      ],
      "description": "Get block data by block hash",
      "returns": "block data in requested format",
      "chain_types": [
        "bitcoin"
      ]
    },
    "getblockchaininfo": {
      "params": [],
      "description": "Get blockchain state info including chain name, blocks, headers, difficulty",
      "returns": "object with comprehensive blockchain state",
      "chain_types": [
        "bitcoin"
      ]
    },
    "getblockhash": {
      "params": [
        "height (number)"
      ],
      "description": "Get block hash for a specific height",
      "returns": "hex string (block hash)",
      "chain_types": [
        "bitcoin"
      ]
    },
    "getblockheader": {
      "params": [
        "blockhash",
        "verbose (boolean)"
      ],
      "description": "Get block header information",
      "returns": "header object or hex string",
      "chain_types": [
        "bitcoin"
      ]
    },
    "getdifficulty": {
      "params": [],
      "description": "Get the current mining difficulty",
      "returns": "number (difficulty)",
      "chain_types": [
        "bitcoin"
      ]
    },
    "getmempoolinfo": {
      "params": [],
      "description": "Get mempool state information",
      "returns": "object with size, bytes, usage, etc.",
      "chain_types": [
        "bitcoin"
      ]
    },
    "getrawmempool": {
      "params": [
        "verbose (boolean, optional)"
      ],
      "description": "Get all transaction IDs in mempool",
      "returns": "array of txids or object with details",
      "chain_types": [
        "bitcoin"
      ]
    },
    "gettxout": {
      "params": [
        "txid",
        "n (output index)",
        "include_mempool (boolean, optional)"
      ],
      "description": "Get details about an unspent transaction output",
      "returns": "object with value, scriptPubKey, etc. or null if spent",
      "chain_types": [
        "bitcoin"
      ]
    }
  }
